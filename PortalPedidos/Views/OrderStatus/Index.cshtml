
@{
    ViewBag.Title = "Index";
    var id = ViewBag.p;
}
<h2>Marcar Reclamo como resuleto</h2>
<br />
<h4>PEDIDO N0. @id</h4>



<div class="col-md-4">
    <section class="form-group">
        @using (Html.BeginForm("Reclamo", "OrderStatus", new { ID_PEDIDO = @id }, FormMethod.Post,
new { enctype = "multipart/form-data", @role = "status" }))
        {
            <button type="submit" name="aceptar" value="aceptarbtn" class="btn btn-lg btn-primary btn-block">Aceptar</button>

        }


    </section>
</div>

<div class="col-md-4">
    <section class="form-group">
        @using (Html.BeginForm("Index", "Reclamations", FormMethod.Post,
new { enctype = "multipart/form-data", @role = "status" }))
        {
            <button type="submit" name="cancelar" value="cancelarbtn" class="btn btn-lg btn-primary btn-block">Cancelar</button>
        }


    </section>
</div>

